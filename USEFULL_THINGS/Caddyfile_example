http://<subdomain>.<domain>, https://<subdomain>.<domain> {
    
    reverse_proxy localhost:8081
    reverse_proxy /api/* localhost:5000
    reverse_proxy /swagger/* localhost:5000
}

# EXAMPLE
http://subdomain.domain.com, https://subdomain.domain.com {
    
    reverse_proxy localhost:8081
    reverse_proxy /api/* localhost:5000
    reverse_proxy /swagger/* localhost:5000
}
